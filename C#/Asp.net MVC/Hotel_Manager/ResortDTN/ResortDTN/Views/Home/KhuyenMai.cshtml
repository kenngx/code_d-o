    <script>
        $(document).ready(function () {
            $("#PageJnewsletterForm").submit(function (e) {
                e.preventDefault();
                $('#btn-submit-newsletter').prop('disabled', true);

                form = $('#PageJnewsletterForm');
                $.ajax({
                    type: "POST",
                    url: "/pages/jnewsletter",
                    data: form.serialize(),
                    success: function (data) {
                        var results = JSON.parse(data);
                        if (results.status == 1) {
                            $.gritter.add({
                                title: 'Thành công',
                                text: results.msg
                            });
                            document.getElementById('jnewletterForm').reset();
                            $('#btn-submit-newsletter').prop('disabled', true);

                        } else {
                            var error = '';
                            results.msg.forEach(function (element) {
                                error = error + element + '<br>';
                            });
                            $.gritter.add({
                                title: 'Lỗi',
                                text: error
                            });
                            $('#btn-submit-newsletter').prop('disabled', false);
                        }
                    }
                });
            })
        })
             })
    </script>	<!-- //Footer -->
