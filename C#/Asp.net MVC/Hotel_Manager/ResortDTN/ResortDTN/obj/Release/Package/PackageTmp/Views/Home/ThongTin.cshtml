                display: flex;
                justify-content: center;
                align-items: center;
            }

                .n-header nav.desktop-menu {
                }

            .n-header .logo-box {
                position: relative;
                height: 80px;
                display: flex;
                justify-content: center;
                align-items: center;
                margin: 15px 0;
                transition: all .3s ease-in-out;
            }

            .n-header.header-scrolldown .logo-box {
                margin: 0;
            }

            .n-header .logo-box .logo img {
                max-width: 100px;
            }

        .n-menu {
            display: flex;
            position: absolute;
            top: 50%;
            transform: translateY(-50%);
            white-space: nowrap;
        }

            .n-menu > li a {
                display: block;
                padding: 0 26px;
                font-weight: 400;
                font-family: 'Roboto Condensed';
                font-size: 13px;
                letter-spacing: 1px;
                color: #f9f9f9;
                text-decoration: none;
                display: inline-block;
                text-transform: uppercase;
            }

                .n-menu > li a:hover, .n-menu > li.active a {
                    color: #c79626;
                }

            .n-menu.menu-1 {
                right: calc(100% + 20px);
            }

            .n-menu.menu-2 {
                left: calc(100% + 20px);
            }

