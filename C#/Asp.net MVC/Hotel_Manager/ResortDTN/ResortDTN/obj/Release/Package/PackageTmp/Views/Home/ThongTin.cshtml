        .n-header {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            z-index: 3;
            background: rgba(48, 48, 48, 0.5);
            transition: all .3s ease-in-out;
        }

            .n-header.header-scrolldown {
                background: rgba(48, 48, 48, .9);
            }

